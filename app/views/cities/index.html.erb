<h2>Click On A City To Get Started</h2>

<main class="container">
  <section class="flex-container">
    <div class="index">
      <% @cities.each do |city| %>
        <a href="/cities/<%= city.id %>">
        <h4><%= city.city %></h4>

        <img class="peek" src="<%= city.image_url %>">
        <br>
        <br>
      <% end %>

    </div>
  </section>
</main>